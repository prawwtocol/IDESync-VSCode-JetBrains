{
  "name": "vscode-jetbrains-sync",
  "displayName": "IDE Sync - Connect to JetBrains IDE (Cmd+I)",
  "version": "1.3.5",
  "publisher": "secret",
  "description": "VSCode extension for synchronizing with JetBrains IDE",
  "icon": "icon.png",
  "sponsor": {
    "url": "https://ko-fi.com/secret"
  },
"keywords": [
    "JetBrains",
    "sync",
    "synchronization",
    "ide",
    "editor",
    "connection",
    "bridge",
    "VSCode",
    "vscode-rider-sync",
    "cursor",
    "windsurf",
    "IntelliJ",
    "intellij-based",
    "IntelliJ IDEA",
    "WebStorm",
    "Rider",
    "JetBrains IDE",
    "IDE Sync",
    "IDE Sync - Connect to VSCode",
    "IDE Sync - Connect to JetBrains IDE"
],
  "repository": {
    "type": "git",
    "url": "https://github.com/secret/IDESync-VSCode-JetBrains"
  },
  "main": "dist/extension.js",
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "commands": [
      {
        "command": "vscode-jetbrains-sync.toggleAutoReconnect",
        "title": "Toggle VSCode-JetBrains Sync",
        "category": "IDE Sync"
      },
      {
        "command": "vscode-jetbrains-sync.switchToPairedIDE",
        "title": "Switch to Paired IDE",
        "category": "IDE Sync"
      },
      {
        "command": "vscode-jetbrains-sync.killDiscoveryServer",
        "title": "Kill Discovery Server",
        "category": "IDE Sync"
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "command": "vscode-jetbrains-sync.toggleAutoReconnect",
          "when": "workspaceFolderCount > 0"
        },
        {
          "command": "vscode-jetbrains-sync.switchToPairedIDE",
          "when": "workspaceFolderCount > 0"
        },
        {
          "command": "vscode-jetbrains-sync.killDiscoveryServer",
          "when": "workspaceFolderCount > 0"
        }
      ]
    },
    "keybindings": [
      {
        "command": "vscode-jetbrains-sync.switchToPairedIDE",
        "key": "cmd+i",
        "mac": "cmd+i",
        "win": "ctrl+i",
        "linux": "ctrl+i"
      }
    ],
    "configuration": {
      "title": "IDE Sync - Connect to JetBrains IDE",
      "properties": {
        "vscode-jetbrains-sync.port": {
          "type": "number",
          "default": 3000,
          "description": "Port number for the WebSocket server"
        }
      }
    }
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "package": "vsce package"
  },
  "dependencies": {
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@types/node": "^20.8.2",
    "@types/vscode": "^1.93.0",
    "@types/ws": "^8.5.10",
    "typescript": "^5.2.2",
    "@vscode/vsce": "^2.24.0"
  },
  "engines": {
    "vscode": "^1.93.0"
  }
}
